{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "builds": [
    {
      "src": "website/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/website/(.*)",
      "dest": "/website/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/website/metastation-hub.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}