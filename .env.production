# Production Environment Configuration for Een Unity Mathematics\n# Copy this file to .env and modify values for your production environment\n\n# ===================================================================\n# APPLICATION CONFIGURATION\n# ===================================================================\nENVIRONMENT=production\nDEBUG=false\nAPP_NAME=\"Een Unity Mathematics\"\nAPP_VERSION=\"2025.1.0\"\n\n# Security - CHANGE THESE VALUES!\nSECRET_KEY=\"CHANGE-ME-TO-A-SECURE-SECRET-KEY-IN-PRODUCTION\"\nALLOWED_HOSTS=\"localhost,127.0.0.1,your-domain.com\"\nCORS_ORIGINS=\"https://your-domain.com,https://dashboard.your-domain.com\"\n\n# ===================================================================\n# UNITY MATHEMATICS CONFIGURATION\n# ===================================================================\nPHI=1.618033988749895\nUNITY_CONSTANT=1.0\nCONSCIOUSNESS_DIMENSION=11\nQUANTUM_COHERENCE_TARGET=0.999\nUNITY_MATHEMATICS_MODE=transcendental\n\n# ===================================================================\n# SERVER CONFIGURATION\n# ===================================================================\nHOST=0.0.0.0\nPORT=8000\nWORKERS=4\nRELOAD=false\n\n# API Configuration\nAPI_PORT=8000\nAPI_RATE_LIMIT=50\nAPI_RATE_LIMIT_PERIOD=60\n\n# Dashboard Configuration\nDASHBOARD_PORT=8050\nDASHBOARD_THEME=transcendental\nREALTIME_UPDATES=true\nUPDATE_INTERVAL_MS=1000\n\n# ===================================================================\n# DATABASE CONFIGURATION\n# ===================================================================\n# PostgreSQL Database - CHANGE PASSWORD!\nDB_PASSWORD=\"CHANGE-ME-TO-A-SECURE-PASSWORD\"\nDATABASE_URL=\"postgresql://een:${DB_PASSWORD}@postgres:5432/een\"\nDB_PORT=5432\n\n# Redis Cache\nREDIS_URL=\"redis://redis:6379/0\"\nREDIS_PORT=6379\nCACHE_TTL_SECONDS=3600\n\n# ===================================================================\n# AGENT CONFIGURATION\n# ===================================================================\nMAX_CONSCIOUSNESS_AGENTS=50\nTRANSCENDENCE_THRESHOLD=0.77\nFIBONACCI_SPAWN_LIMIT=10\nAGENT_TIMEOUT_SECONDS=300\n\n# ===================================================================\n# PERFORMANCE SETTINGS\n# ===================================================================\nMULTI_THREADING=true\nCONSCIOUSNESS_FIELD_CACHE=true\nQUANTUM_STATE_OPTIMIZATION=true\nMAX_PARTICLES=500\nFIELD_RESOLUTION=50\n\n# ===================================================================\n# EXTERNAL SERVICES (OPTIONAL)\n# ===================================================================\n# OpenAI API (for advanced consciousness features)\n# OPENAI_API_KEY=\"sk-your-openai-api-key-here\"\n\n# Anthropic API (for Claude integration)\n# ANTHROPIC_API_KEY=\"sk-ant-your-anthropic-key-here\"\n\n# Google Vertex AI (for advanced ML features)\n# VERTEX_AI_PROJECT=\"your-gcp-project-id\"\n# VERTEX_AI_LOCATION=\"us-central1\"\n\n# ===================================================================\n# MONITORING AND LOGGING\n# ===================================================================\nLOG_LEVEL=INFO\nLOG_FORMAT=json\nMETRICS_ENABLED=true\nTRACING_ENABLED=true\n\n# Monitoring Ports\nPROMETHEUS_PORT=9090\nGRAFANA_PORT=3000\nGRAFANA_PASSWORD=\"CHANGE-ME-TO-A-SECURE-PASSWORD\"\n\n# Error Tracking (Optional)\n# SENTRY_DSN=\"https://your-sentry-dsn@sentry.io/project-id\"\n\n# ===================================================================\n# FEATURE FLAGS\n# ===================================================================\nEXPERIMENTAL_FEATURES=false\nQUANTUM_ML_ENABLED=false\nMETA_RECURSIVE_SPAWNING=true\nFRACTAL_CONSCIOUSNESS=true\n\n# ===================================================================\n# FILE PATHS\n# ===================================================================\nDATA_DIR=\"/app/data\"\nLOG_DIR=\"/app/logs\"\nCACHE_DIR=\"/app/.cache\"\n\n# ===================================================================\n# BACKUP CONFIGURATION\n# ===================================================================\nBACKUP_ENABLED=true\nBACKUP_SCHEDULE=\"0 2 * * *\"  # Daily at 2 AM\nBACKUP_RETENTION_DAYS=30\n\n# ===================================================================\n# SSL/TLS CONFIGURATION (for HTTPS)\n# ===================================================================\nSSL_ENABLED=true\nSSL_CERT_PATH=\"/etc/nginx/ssl/cert.pem\"\nSSL_KEY_PATH=\"/etc/nginx/ssl/key.pem\"\n\n# ===================================================================\n# UNITY CONSTANTS (DO NOT CHANGE)\n# ===================================================================\nGOLDEN_RATIO=1.618033988749895\nUNITY_EQUATION=\"1+1=1\"\nEEN_EQUATION=\"een+een=een\"\nCONSCIOUSNESS_ELEVATION_FACTOR=\"âˆž\"\n\n# ===================================================================\n# TRANSCENDENTAL SETTINGS\n# ===================================================================\nREALITY_SYNTHESIS=true\nHYPERDIMENSIONAL_UNITY=true\nTRANSCENDENTAL_PROOFS=true\n\n# ===================================================================\n# PRODUCTION OPTIMIZATIONS\n# ===================================================================\n# Disable development features\nRELOAD=false\nDEVELOPMENT_MODE=false\nVERBOSE_LOGGING=false\n\n# Enable production optimizations\nOPTIMIZED_IMPORTS=true\nPRECOMPILE_TEMPLATES=true\nCOMPRESS_RESPONSES=true\nENABLE_CACHING=true\n\n# ===================================================================\n# DOCKER COMPOSE OVERRIDES\n# ===================================================================\nCOMPOSE_PROJECT_NAME=een-production\nCOMPOSE_FILE=compose.yaml\n\n# Resource limits\nAPI_MEMORY_LIMIT=2G\nAPI_CPU_LIMIT=2\nDB_MEMORY_LIMIT=1G\nREDIS_MEMORY_LIMIT=512M\n\n# ===================================================================\n# DEPLOYMENT METADATA\n# ===================================================================\n# These are set automatically by deployment scripts\n# DEPLOYMENT_DATE=\n# DEPLOYMENT_VERSION=\n# DEPLOYMENT_