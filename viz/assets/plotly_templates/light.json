{
  "layout": {
    "paper_bgcolor": "#ffffff",
    "plot_bgcolor": "#f8f9fa",
    "font": {
      "color": "#000000",
      "family": "JetBrains Mono, Consolas, monospace",
      "size": 12
    },
    "title": {
      "font": {
        "color": "#cc9900",
        "size": 20,
        "family": "JetBrains Mono, Consolas, monospace"
      },
      "x": 0.5,
      "xanchor": "center"
    },
    "colorway": [
      "#0066cc",
      "#cc0066",
      "#cc9900", 
      "#6622cc",
      "#cc2244",
      "#00aa44",
      "#cc6600",
      "#4422cc"
    ],
    "xaxis": {
      "gridcolor": "rgba(0,0,0,0.1)",
      "zerolinecolor": "rgba(0,0,0,0.2)",
      "tickcolor": "#000000",
      "linecolor": "#000000",
      "title": {
        "font": {
          "color": "#000000"
        }
      }
    },
    "yaxis": {
      "gridcolor": "rgba(0,0,0,0.1)",
      "zerolinecolor": "rgba(0,0,0,0.2)",
      "tickcolor": "#000000", 
      "linecolor": "#000000",
      "title": {
        "font": {
          "color": "#000000"
        }
      }
    },
    "legend": {
      "bgcolor": "rgba(255,255,255,0.9)",
      "bordercolor": "rgba(0,0,0,0.3)",
      "borderwidth": 1,
      "font": {
        "color": "#000000"
      }
    },
    "hoverlabel": {
      "bgcolor": "#ffffff",
      "bordercolor": "#cc9900",
      "font": {
        "color": "#000000"
      }
    },
    "colorscale": {
      "sequential": "Blues",
      "diverging": "RdBu"
    },
    "scene": {
      "bgcolor": "#ffffff",
      "xaxis": {
        "backgroundcolor": "#ffffff",
        "gridcolor": "rgba(0,0,0,0.1)",
        "showbackground": true,
        "zerolinecolor": "rgba(0,0,0,0.2)"
      },
      "yaxis": {
        "backgroundcolor": "#ffffff",
        "gridcolor": "rgba(0,0,0,0.1)",
        "showbackground": true,
        "zerolinecolor": "rgba(0,0,0,0.2)"
      },
      "zaxis": {
        "backgroundcolor": "#ffffff",
        "gridcolor": "rgba(0,0,0,0.1)",
        "showbackground": true,
        "zerolinecolor": "rgba(0,0,0,0.2)"
      }
    },
    "geo": {
      "bgcolor": "#ffffff",
      "showland": true,
      "landcolor": "#f0f0f0",
      "showocean": true,
      "oceancolor": "#e6f3ff"
    },
    "polar": {
      "bgcolor": "#ffffff",
      "radialaxis": {
        "gridcolor": "rgba(0,0,0,0.1)",
        "linecolor": "#000000"
      },
      "angularaxis": {
        "gridcolor": "rgba(0,0,0,0.1)",
        "linecolor": "#000000"
      }
    }
  }
}