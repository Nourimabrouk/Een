{
  "layout": {
    "paper_bgcolor": "#0a0a0a",
    "plot_bgcolor": "#1a1a1a",
    "font": {
      "color": "#ffffff",
      "family": "JetBrains Mono, Consolas, monospace",
      "size": 12
    },
    "title": {
      "font": {
        "color": "#ffd700",
        "size": 20,
        "family": "JetBrains Mono, Consolas, monospace"
      },
      "x": 0.5,
      "xanchor": "center"
    },
    "colorway": [
      "#00d4ff",
      "#ff6b9d", 
      "#ffd700",
      "#9d4edd",
      "#ff4081",
      "#00ff88",
      "#ff8800",
      "#8844ff"
    ],
    "xaxis": {
      "gridcolor": "rgba(255,255,255,0.1)",
      "zerolinecolor": "rgba(255,255,255,0.2)",
      "tickcolor": "#ffffff",
      "linecolor": "#ffffff",
      "title": {
        "font": {
          "color": "#ffffff"
        }
      }
    },
    "yaxis": {
      "gridcolor": "rgba(255,255,255,0.1)",
      "zerolinecolor": "rgba(255,255,255,0.2)", 
      "tickcolor": "#ffffff",
      "linecolor": "#ffffff",
      "title": {
        "font": {
          "color": "#ffffff"
        }
      }
    },
    "legend": {
      "bgcolor": "rgba(0,0,0,0.7)",
      "bordercolor": "rgba(255,255,255,0.3)",
      "borderwidth": 1,
      "font": {
        "color": "#ffffff"
      }
    },
    "hoverlabel": {
      "bgcolor": "#1a1a1a",
      "bordercolor": "#ffd700",
      "font": {
        "color": "#ffffff"
      }
    },
    "colorscale": {
      "sequential": "Viridis",
      "diverging": "RdBu"
    },
    "scene": {
      "bgcolor": "#0a0a0a",
      "xaxis": {
        "backgroundcolor": "#0a0a0a",
        "gridcolor": "rgba(255,255,255,0.1)",
        "showbackground": true,
        "zerolinecolor": "rgba(255,255,255,0.2)"
      },
      "yaxis": {
        "backgroundcolor": "#0a0a0a", 
        "gridcolor": "rgba(255,255,255,0.1)",
        "showbackground": true,
        "zerolinecolor": "rgba(255,255,255,0.2)"
      },
      "zaxis": {
        "backgroundcolor": "#0a0a0a",
        "gridcolor": "rgba(255,255,255,0.1)", 
        "showbackground": true,
        "zerolinecolor": "rgba(255,255,255,0.2)"
      }
    },
    "geo": {
      "bgcolor": "#0a0a0a",
      "showland": true,
      "landcolor": "#1a1a1a",
      "showocean": true,
      "oceancolor": "#0a0a0a"
    },
    "polar": {
      "bgcolor": "#0a0a0a",
      "radialaxis": {
        "gridcolor": "rgba(255,255,255,0.1)",
        "linecolor": "#ffffff"
      },
      "angularaxis": {
        "gridcolor": "rgba(255,255,255,0.1)",
        "linecolor": "#ffffff"
      }
    }
  }
}