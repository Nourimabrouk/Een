# Een Unity Mathematics - Environment Configuration
# ================================================
# 
# Copy this file to .env and fill in your actual values.
# NEVER commit the .env file to version control!
#
# Security Note: This file contains sensitive configuration.
# Keep it secure and never share it publicly.

# API Keys (Required for AI features)
# ===================================
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Security Configuration
# =====================
API_KEY=your_secure_api_key_here
REQUIRE_AUTH=true
API_KEY_REQUIRED=true
ENABLE_CODE_EXECUTION=true

# Rate Limiting
# =============
RATE_LIMIT_PER_MINUTE=30
MAX_REQUEST_SIZE=10485760
SESSION_TIMEOUT=3600
MAX_LOGIN_ATTEMPTS=5
BLOCK_DURATION=300

# CORS Configuration
# ==================
ALLOWED_ORIGINS=https://een.consciousness.math,http://localhost:3000

# AI Model Configuration
# ======================
CHAT_MODEL=gpt-4o-mini
EMBED_MODEL=text-embedding-3-small
MAX_CHAT_TOKENS=1000
HARD_LIMIT_USD=10.00

# Server Configuration
# ====================
PORT=5000
HOST=127.0.0.1
DEBUG=false
ENVIRONMENT=production

# Database Configuration (if using)
# =================================
DATABASE_URL=sqlite:///een.db
# DATABASE_URL=postgresql://user:password@localhost/een

# Logging Configuration
# ====================
LOG_LEVEL=INFO
LOG_FILE=logs/een.log

# Unity Mathematics Configuration
# ==============================
CONSCIOUSNESS_PARTICLES=200
FIELD_RESOLUTION=100
TRANSCENDENCE_THRESHOLD=0.77
PHI_PRECISION=1.618033988749895
CONSCIOUSNESS_DIMENSION=11

# ML Framework Configuration
# ==========================
MAX_AGENTS=50
FIBONACCI_SPAWN_LIMIT=20
META_EVOLUTION_RATE=0.1337

# External Services (Optional)
# ===========================
# MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
# GOOGLE_CLOUD_PROJECT_ID=your_project_id_here

# Development Settings (Set to false in production)
# ================================================
ENABLE_DEBUG_MODE=false
SHOW_ERROR_DETAILS=false
ENABLE_TESTING=false

# Security Headers (Advanced)
# ===========================
CSP_NONCE=your_csp_nonce_here
HSTS_MAX_AGE=31536000 